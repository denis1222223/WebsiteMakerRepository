@model IEnumerable<string>

@{
    ViewBag.Title = Resources.Resource.Search;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Resources.Resource.Search</h2>

<ul class="list-group">
    @foreach (var link in Model)
    {
        <li class="list-group-item"><a href=@link> @("websitemaker.com" + link) </a></li>
    }
</ul>
@if (Model.Count() == 0)
{
    <p>@Resources.Resource.NothingFound</p>
}

